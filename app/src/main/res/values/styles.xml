<?xml version="1.0" encoding="utf-8"?>

<resources>
    <style name="DialogAnimation">
<!--        <item name="android:windowEnterAnimation">@anim/slide_up</item>-->
<!--        <item name="android:windowExitAnimation">@anim/slide_down</item>-->
    </style>

    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/accent</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/accent</item>
        <item name="colorControlHighlight">@color/accent</item>
        <item name="colorControlActivated">@color/accent</item>
        <item name="android:textColorPrimary">@color/accent</item>
        <item name="colorOnSurface">@color/colorPrimaryLight2</item>
        <item name="android:navigationBarColor">@color/colorPrimary</item>
        <item name="android:windowBackground">@color/colorPrimary</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item>

    </style>

    <style name="MyButton" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="colorPrimary">@color/button</item>
        <item name="android:textColor">@color/primary_text</item>
    </style>
    <style name="MyButtoncustom" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="colorPrimary">@color/colorPrimaryLight4</item>
        <item name="android:textColor">@color/primary_text_custom</item>
    </style>

    <style name="MyTab" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:background">@color/colorPrimary</item>
    </style>

    <!-- Splash theme. -->
    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_background</item>
    </style>

    <style name="usbSpinnerDialog">
        <item name="android:background">@drawable/shotcut_background_gold</item>
    </style>

    <style name="customSpinnerDialog">
        <item name="android:background">@color/colorPrimaryLight</item>
    </style>

    <style name="AppTheme.Spinner">
        <item name="android:spinnerItemStyle">@style/SpinnerItem</item>
        <item name="android:spinnerDropDownItemStyle">@style/SpinnerDropDownItem</item>
    </style>

    <style name="SpinnerItem">
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:background">@color/colorPrimaryLight</item>

    </style>

    <style name="SpinnerDropDownItem">
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:background">@color/colorPrimaryLight</item>
    </style>
    <style name="search_stryle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/teal</item>
    </style>
